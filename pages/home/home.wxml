<!--pages/home/home.wxml-->
<!-- 1.navigator页面跳转 -->
<navigator url="/pages/detail/detail">跳到详情页</navigator>

<!-- 2.navigator属性：open-type=‘redirect’ -->
<navigator url="/pages/detail/detail" open-type="redirect">跳到详情页redirect</navigator>

<!-- 3.navigator属性：open-type=‘’ -->
<navigator url="/pages/about/about" open-type="switchTab">跳到详情页switchTab</navigator>

<!-- 4.navigator属性：open-type=‘reLaunch’ -->
<navigator url="/pages/detail/detail" open-type="reLaunch">跳到详情页reLaunch</navigator>

<!-- 5.navigator跳转过程中数据的传递 -->
<navigator url="/pages/detail/detail?name=jerry&age=18&height=1.88">跳到详情页</navigator>

<view>{{title}}</view>

<!-- 6.通过代码进行页面跳转 -->
<button size="mini" bind:tap="handlePushDetail">跳转详情页</button>


